{
    "gitlens.menus": {
        "editor": {
            "blame": false,
            "clipboard": true,
            "compare": true,
            "details": false,
            "history": false,
            "remote": false
        },
        "editorGroup": {
            "blame": true,
            "compare": true
        },
        "editorTab": {
            "clipboard": true,
            "compare": true,
            "history": true,
            "remote": true
        },
        "explorer": {
            "clipboard": true,
            "compare": true,
            "history": true,
            "remote": true
        },
        "scmGroup": {
            "compare": true,
            "openClose": true,
            "stash": true,
            "stashInline": true
        }
    },
    "files.insertFinalNewline": true,
    "editor.rederControlCharacters": true,
    "breadcrumbs.enabled": true,
    "editor.fontSize": 14,
    "workbench.tree.indent": 16,
    "workbench.editor.enablePreview": false,
    "workbench.editor.enablePreviewFromQuickOpen": false,
    "workbench.editor.highlightModifiedTabs": true,
    "workbench.editor.swipeToNavigate": true,
    "search.showLineNumbers": true,
    "eslint.packageManager": "yarn",
    "gitlens.codeLens.recentChange.enabled": false,
    "vim.hlsearch": true,
    "vim.visualstar": true,
    "gitlens.hovers.currentLine.over": "line",
    "gitlens.codeLens.enabled": false,
    "workbench.statusBar.feedback.visible": false,
    "gitlens.currentLine.enabled": false,
    "editor.tabSize": 2,
    "cSpell.language": "en",
    "cSpell.userWords": [
        "APIV",
        "Chakra",
        "classname",
        "cloudfront",
        "Commentable",
        "component",
        "denormalize",
        "docgen",
        "Dropzone",
        "e",
        "eightcard",
        "enquete",
        "etestid",
        "eyecatch",
        "Formik",
        "hotfix",
        "momentize",
        "myhome",
        "Navi",
        "noopener",
        "noreferrer",
        "normalizr",
        "Onair",
        "openapi",
        "presigned",
        "Reactable",
        "refetch",
        "Sansan",
        "Serializable",
        "Storyshots",
        "stylelint",
        "testid",
        "unmount",
        "Uploader",
        "waait",
        "will"
    ],
    "search.followSymlinks": false,
    "path-intellisense.mappings": {
        "actions": "${workspaceRoot}/www/front/src/actions",
        "components": "${workspaceRoot}/www/front/src/components",
        "constants": "${workspaceRoot}/www/front/src/constants",
        "initializer": "${workspaceRoot}/www/front/src/initializer",
        "locales": "${workspaceRoot}/www/front/src/locales",
        "loggers": "${workspaceRoot}/www/front/src/loggers",
        "middlewares": "${workspaceRoot}/www/front/src/middlewares",
        "models": "${workspaceRoot}/www/front/src/models",
        "polyfills": "${workspaceRoot}/www/front/src/polyfills",
        "roots": "${workspaceRoot}/www/front/src/roots",
        "schemas": "${workspaceRoot}/www/front/src/schemas",
        "styles_misc": "${workspaceRoot}/www/front/src/styles_misc",
        "utils": "${workspaceRoot}/www/front/src/utils"
    },
    "javascript.preferences.importModuleSpecifier": "non-relative",
    "typescript.preferences.importModuleSpecifier": "non-relative",
    "javascript.updateImportsOnFileMove.enabled": "never",
    "editor.formatOnType": true,
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": true,
        "source.fixAll.stylelint": true
    },
    "typescript.suggest.autoImports": false,
    "diffEditor.ignoreTrimWhitespace": false,
    "ruby.intellisense": "rubyLocate",
    "liveshare.featureSet": "insiders",
    "vsicons.dontShowNewVersionMessage": true,
    "peacock.favoriteColors": [
        {
            "name": "Angular Red",
            "value": "#b52e31"
        },
        {
            "name": "Auth0 Orange",
            "value": "#eb5424"
        },
        {
            "name": "Azure Blue",
            "value": "#007fff"
        },
        {
            "name": "C# Purple",
            "value": "#68217A"
        },
        {
            "name": "Gatsby Purple",
            "value": "#639"
        },
        {
            "name": "Go Cyan",
            "value": "#5dc9e2"
        },
        {
            "name": "Java Blue-Gray",
            "value": "#557c9b"
        },
        {
            "name": "JavaScript Yellow",
            "value": "#f9e64f"
        },
        {
            "name": "Mandalorian Blue",
            "value": "#1857a4"
        },
        {
            "name": "Node Green",
            "value": "#215732"
        },
        {
            "name": "React Blue",
            "value": "#00b3e6"
        },
        {
            "name": "Something Different",
            "value": "#832561"
        },
        {
            "name": "Vue Green",
            "value": "#42b883"
        }
    ],
    "workbench.iconTheme": "material-icon-theme",
    "typescript.updateImportsOnFileMove.enabled": "never",
    "yaml.schemas": {
      "https://raw.githubusercontent.com/SchemaStore/schemastore/master/src/schemas/json/github-workflow.json": [
        ".github/workflows/*.yml",
        ".github/workflows/*.yaml"
      ]
    },
    "files.associations": {
        "*.css.erb": "css"
    },
    "editor.minimap.enabled": false,
    "auto-close-tag.activationOnLanguage": [
        "xml",
        "php",
        "blade",
        "ejs",
        "jinja",
        "javascript",
        "javascriptreact",
        "typescript",
        "typescriptreact",
        "plaintext",
        "markdown",
        "vue",
        "liquid",
        "erb",
        "lang-cfml",
        "cfml",
        "HTML (EEx)",
        "HTML (Eex)",
        "plist"
    ],
    "redhat.telemetry.enabled": true,
    "editor.cursorStyle": "line",
    "editor.lineNumbers": "on",
    "editor.wordSeparators": "/\\()\"':,.;<>~!@#$%^&*|+=[]{}`?-",
    "editor.wordWrap": "off",
    "editor.guides.bracketPairs": "active",
    "search.exclude": {
        "**/.build/**": true
    },
    "files.exclude": {
        "**/*.tsbuildinfo": true
    }
}
